<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- This is to be set as a background
    and an outline provider for BlurView-->
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="rectangle">
            <corners android:radius="16dp" />
            <solid android:color="#99141A1A" />
        </shape>
    </item>
</layer-list>
